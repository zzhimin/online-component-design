else
  export NODE_PATH="/mnt/d/project/juejin/online-component-design/node_modules/.pnpm/@angular+compiler-cli@15.2.10_@angular+compiler@15.2.10_@angular+core@15.2.10_rxjs@7.8.1_zone_z6mwdm42s4gtf4hqvhgq3j62de/node_modules/@angular/compiler-cli/bundles/src/bin/node_modules:/mnt/d/project/juejin/online-component-design/node_modules/.pnpm/@angular+compiler-cli@15.2.10_@angular+compiler@15.2.10_@angular+core@15.2.10_rxjs@7.8.1_zone_z6mwdm42s4gtf4hqvhgq3j62de/node_modules/@angular/compiler-cli/bundles/src/node_modules:/mnt/d/project/juejin/online-component-design/node_modules/.pnpm/@angular+compiler-cli@15.2.10_@angular+compiler@15.2.10_@angular+core@15.2.10_rxjs@7.8.1_zone_z6mwdm42s4gtf4hqvhgq3j62de/node_modules/@angular/compiler-cli/bundles/node_modules:/mnt/d/project/juejin/online-component-design/node_modules/.pnpm/@angular+compiler-cli@15.2.10_@angular+compiler@15.2.10_@angular+core@15.2.10_rxjs@7.8.1_zone_z6mwdm42s4gtf4hqvhgq3j62de/node_modules/@angular/compiler-cli/node_modules:/mnt/d/project/juejin/online-component-design/node_modules/.pnpm/@angular+compiler-cli@15.2.10_@angular+compiler@15.2.10_@angular+core@15.2.10_rxjs@7.8.1_zone_z6mwdm42s4gtf4hqvhgq3j62de/node_modules/@angular/node_modules:/mnt/d/project/juejin/online-component-design/node_modules/.pnpm/@angular+compiler-cli@15.2.10_@angular+compiler@15.2.10_@angular+core@15.2.10_rxjs@7.8.1_zone_z6mwdm42s4gtf4hqvhgq3j62de/node_modules:/mnt/d/project/juejin/online-component-design/node_modules/.pnpm/node_modules:$NODE_PATH"
