#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/project/juejin/online-component-design/node_modules/.pnpm/pacote@15.1.0/node_modules/pacote/lib/node_modules:/mnt/d/project/juejin/online-component-design/node_modules/.pnpm/pacote@15.1.0/node_modules/pacote/node_modules:/mnt/d/project/juejin/online-component-design/node_modules/.pnpm/pacote@15.1.0/node_modules:/mnt/d/project/juejin/online-component-design/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/project/juejin/online-component-design/node_modules/.pnpm/pacote@15.1.0/node_modules/pacote/lib/node_modules:/mnt/d/project/juejin/online-component-design/node_modules/.pnpm/pacote@15.1.0/node_modules/pacote/node_modules:/mnt/d/project/juejin/online-component-design/node_modules/.pnpm/pacote@15.1.0/node_modules:/mnt/d/project/juejin/online-component-design/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../pacote@15.1.0/node_modules/pacote/lib/bin.js" "$@"
else
  exec node  "$basedir/../../../../../../pacote@15.1.0/node_modules/pacote/lib/bin.js" "$@"
fi
